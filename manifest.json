{
    "manifest_version": 2,

    "name": "SurfShield",
    "description": "This extension will monitor the safety of a website",
    "version": "0.0.1",

    "browser_action": {
        "default_icon": "info.png",
        "default_title": "SurfShield",
        "default_popup": "main.html"
    },

    "permissions": [
    "tabs",
    "activeTab",  
    "https://ajax.googleapis.com/"
    ]
}